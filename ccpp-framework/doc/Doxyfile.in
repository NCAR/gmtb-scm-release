PROJECT_NAME           = "Common Community Physics Package"#"@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
FILE_PATTERNS          = *.md  \
                         *.h   \
                         *.c   \
                         *.f90 \
                         *.F90
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/README.md
OPTIMIZE_FOR_FORTRAN   = YES
INLINE_SIMPLE_STRUCTS  = YES
EXTRACT_ALL            = YES
HAVE_DOT               = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
PROJECT_LOGO           = @PROJECT_SOURCE_DIR@/doc/img/dtc_logo.png
DISABLE_INDEX          = YES
GENERATE_TREEVIEW      = YES
